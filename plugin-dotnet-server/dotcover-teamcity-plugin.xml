<?xml version="1.0" encoding="UTF-8"?>
<teamcity-agent-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                       xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-agent-plugin-v1-xml">
  <tool-deployment>
    <layout>
      <executable-files>
        <include name="linux-arm/JetBrains.Profiler.PdbServer" />
        <include name="linux-arm64/JetBrains.Profiler.PdbServer" />
        <include name="linux-musl-arm64/JetBrains.Profiler.PdbServer" />
        <include name="linux-musl-x64/JetBrains.Profiler.PdbServer" />
        <include name="linux-x64/JetBrains.Profiler.PdbServer" />
        <include name="macos-arm64/JetBrains.Profiler.PdbServer" />
        <include name="macos-x64/JetBrains.Profiler.PdbServer" />

        <!-- only for the deprecated cross-platform JetBrains.dotCover.DotNetCli NuGet package-->
        <include name="dotCover.sh" />
        <include name="runtime-dotnet.sh" />
        <include name="runtime.sh" />
        <include name="linux-arm/dotnet/dotnet" />
        <include name="linux-arm/dotnet/crossgen2/crossgen2" />
        <include name="linux-arm/dotnet/shared/Microsoft.NETCore.App/6.0.6/createdump" />
        <include name="linux-arm/dotnet/shared/Microsoft.NETCore.App/7.0.10/createdump" />
        <include name="linux-arm64/dotnet/dotnet" />
        <include name="linux-musl-arm64/dotnet/dotnet" />
        <include name="linux-musl-x64/dotnet/dotnet" />
        <include name="linux-x64/dotnet/dotnet" />
        <include name="linux-x64/dotnet/crossgen2/crossgen2" />
        <include name="linux-x64/dotnet/shared/Microsoft.NETCore.App/6.0.6/createdump" />
        <include name="linux-x64/dotnet/shared/Microsoft.NETCore.App/7.0.10/createdump" />
        <include name="linux-x64/mono/bin/mono-sgen" />
        <include name="macos-x64/dotnet/dotnet" />
        <include name="macos-x64/dotnet/crossgen2/crossgen2" />
        <include name="macos-x64/dotnet/shared/Microsoft.NETCore.App/6.0.6/createdump" />
        <include name="macos-x64/dotnet/shared/Microsoft.NETCore.App/7.0.10/createdump" />

        <!-- leaved for sake of compatibility; remove when support of deprecated JetBrains.dotCover.DotNetCli will be discontinued -->
        <include name="dotCover.exe" />
        <include name="windows-x86/JetBrains.Profiler.PdbServer.exe" />
        <include name="windows-x64/JetBrains.Profiler.PdbServer.exe" />
      </executable-files>
    </layout>
  </tool-deployment>
</teamcity-agent-plugin>